{
  "version": 2,
  "builds": [
    {
      "src": "api/places.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/places",
      "dest": "/api/places.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/places.js",
      "methods": ["GET"]
    }
  ],
  "env": {
    "GOOGLE_PLACES_API_KEY": "@google_places_api_key"
  }
}
